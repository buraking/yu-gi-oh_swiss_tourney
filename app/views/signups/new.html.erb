<%= form_for @user,url: signups_path do |f| %>
    <%= label_tag(:n, "Name: ") %>
    <%= f.text_field :name %><br>
    <%= label_tag(:n, "Email: ") %>
    <%= f.text_field :email %><br>
    <%= f.object.errors.full_messages.join(", ") if f.object.errors.any?%><br>
    <%= submit_tag 'Sign up' %>
    <%end%>

<%= button_to "Back to sign in", root_path, method: "get" %>
